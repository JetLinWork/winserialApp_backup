<Window x:Class="demo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:demo"
        mc:Ignorable="d"
        Title="物联网参数调试工具 V1.1" Height="800" Width="1310" ResizeMode="CanMinimize" Closing="Window_Closing">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="200"/>
            <RowDefinition Height="400*"/>
            <RowDefinition Height="150*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="79.5"/>
            <ColumnDefinition Width="190.5"/>
            <ColumnDefinition Width="930*"/>
            <ColumnDefinition Width="150*"/>
        </Grid.ColumnDefinitions>
        <GroupBox Header="串口参数设置" BorderBrush="Black"  Margin="5" Grid.ColumnSpan="2">
            <StackPanel >
                <StackPanel Orientation="Horizontal">
                    <TextBlock Text="串口号" Width="auto" Margin="5" Padding="0,6,0,0"/>
                    <ComboBox Name="cb_SerialPortNumber" Width="75" Margin="5,10,5,10" />
                    <Button Name="PortRefresh" Content="刷新" Margin="15,10,10,10" Width="30" Background="Yellow" Click="SeriPortRefresh"></Button>
                </StackPanel>
                <StackPanel Orientation="Horizontal">
                    <TextBlock Text="校验位" Width="auto" Margin="5" Padding="0,6,0,0"/>
                    <ComboBox Name="cb_Check" Width="65" Margin="5,10,5,10" SelectedIndex="0">
                        <ComboBoxItem Content="None"/>
                    </ComboBox>
                    <TextBlock Text="数据位" Width="auto" Margin="5" Padding="0,6,0,0"/>
                    <ComboBox Name="cb_DataBits" Width="68" Margin="10,10,0,10" SelectedIndex="0">
                        <ComboBoxItem Content="8"/>
                    </ComboBox>
                </StackPanel>
                <StackPanel Orientation="Horizontal">
                    <TextBlock Text="停止位" Width="auto" Margin="10,10,0,10" Padding="0,3,0,0"/>
                    <ComboBox Name="cb_StopBits" Width="65" Margin="5,10,5,10" SelectedIndex="0">
                        <ComboBoxItem Content="one"/>
                    </ComboBox>
                    <TextBlock Text="波特率" Width="auto" Margin="5" Padding="0,6,0,0"/>
                    <ComboBox Name="cb_BaudRate" Width="70" Margin="10,10,0,10" SelectedIndex="0">
                        <!--<ComboBoxItem Content="9600"/>-->
                        <ComboBoxItem Content="115200"/>
                    </ComboBox>
                </StackPanel>
                <StackPanel Orientation="Horizontal">
                    <TextBlock Name="tb_switchStatus" Text="串口为关闭状态" Width="auto" Margin="10" Padding="0,2,0,0"/>
                    <Ellipse Name="e_status" Fill="#000000" Height="10" Stroke="Black" Margin="5,3,10,0" Width="10" />
                    <Button Name="bt_SerialSwitch" Content="打开串口" Margin="10" Padding="10,0,10,0" Click="bt_SerialSwitch_Click"/>

                </StackPanel>
            </StackPanel>
        </GroupBox>


        <GroupBox Header="接收数据(Str/Hex)" Grid.Row="1" Grid.RowSpan="2" BorderBrush="Black"  Margin="5" Grid.ColumnSpan="2">
            <StackPanel >
                <Border  BorderBrush="Black" BorderThickness="1">
                    <StackPanel Orientation="Horizontal" Height="480">
                        <ScrollViewer Name="RecieveViewer" VerticalScrollBarVisibility="Auto"  Width="240">
                            <TextBox Name="tb_receiveData" IsReadOnly="True" TextWrapping="Wrap" />
                        </ScrollViewer>
                    </StackPanel>
                </Border>
                <StackPanel Orientation="Horizontal">
                    <Button Name="ClearReceiveData" Content="清空接收数据" Margin="10,20,10,20" Click="ClearReceiveData_Click"/>
                    <Button Name="StopSend" Content="停止接收" Margin="10,20,10,20" Padding="15,0,15,0" Click="StopSend_Click"/>
                    <Button Name="DisplaySwich" Content="Str" Width="35" Margin="10,20,10,20" Padding="5,0,5,0" Click="DisplaySwich_Click"/>
                </StackPanel>
            </StackPanel>
        </GroupBox>



        <GroupBox Header="提示" Grid.Column="2" Grid.RowSpan="1" Grid.ColumnSpan="1" BorderBrush="Black"  Margin="5">
            <StackPanel  Orientation="Horizontal" Height="170">
                <ScrollViewer VerticalScrollBarVisibility="Auto"  Width="865">
                    <TextBox Name="tb_ResultData" Text="请先打开串口；" IsReadOnly="True"  TextWrapping="Wrap" FontSize="30"/>
                </ScrollViewer>
            </StackPanel>
        </GroupBox>

        <GroupBox Header="PLC寄存器设置" Grid.Row="1" Grid.Column="2" Grid.RowSpan="2" BorderBrush="Black"  Margin="5">
            <StackPanel>
                <Border BorderBrush="Black" BorderThickness="2">
                    <StackPanel Orientation="Horizontal"  HorizontalAlignment="Left">
                        <Border  BorderBrush="Gray" BorderThickness="1" Width="730" Margin="3">
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="选择PLC型号：" FontSize="14" VerticalAlignment="Center" Margin="20,10,10,10"></TextBlock>
                                <Button Name="bt_DeltaSelect" Background="Red" Margin="20,10,10,10" Padding="5" Content="台达PLC" Width="80" Click="Bt_DeltaSelect_Click"></Button>
                                <Button Name="bt_InvertSelect" Margin="20,10,10,10" Padding="5" Content="英威腾PLC" Width="80" Click="Bt_InvertSelect_Click"></Button>
                                <Button Name="bt_SaveData" Margin="190,15,10,10" Padding="1" Content="保存配置" Width="80" Click="Bt_SaveData_Click"></Button>
                                <Button Name="bt_LoadData" Margin="10,15,10,10" Padding="1" Content="读取配置" Width="80" Click="Bt_LoadData_Click"></Button>
                            </StackPanel>
                        </Border>
                        <Button Name="bt_ParaSet" Margin="30,10,10,10" Padding="5" Content="设置寄存器" Width="80" Background="Red" Click="Bt_ParaSet_Click"/>
                    </StackPanel>
                </Border>

                <StackPanel  Name="DeltaPLC" Visibility="Visible" Height="470">
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Margin="20,10,0,5" Width="123" Text="模式一设定长度   D" FontSize="14" VerticalAlignment="Center"></TextBlock>
                        <TextBox Name="DR1" Height="23" TextWrapping="Wrap" Text="" Margin="0,10,10,5" Width="60" VerticalContentAlignment="Center" PreviewKeyDown="TBox_KeyPress" MaxLength="4" TextChanged="TBox_TextChanged"/>
                        <TextBlock Margin="20,10,0,5" Width="123" Text="模式二设定长度   D" FontSize="14" VerticalAlignment="Center"></TextBlock>
                        <TextBox Name="DR2" Height="23" TextWrapping="Wrap" Text="" Margin="0,10,10,5" Width="60" VerticalContentAlignment="Center" PreviewKeyDown="TBox_KeyPress" MaxLength="4" TextChanged="TBox_TextChanged"/>
                        <TextBlock Margin="20,10,0,5" Width="123" Text="模式三设定长度   D" FontSize="14" VerticalAlignment="Center"></TextBlock>
                        <TextBox Name="DR3" Height="23" TextWrapping="Wrap" Text="" Margin="0,10,10,5" Width="60" VerticalContentAlignment="Center" PreviewKeyDown="TBox_KeyPress" MaxLength="4" TextChanged="TBox_TextChanged"/>
